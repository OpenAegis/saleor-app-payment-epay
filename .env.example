# Saleor App 配置
APP_URL=http://localhost:3000
STOREFRONT_URL=https://shop.lzsm.shop

# Upstash Redis 配置（多站点共享数据）
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-redis-token

# 插件管理员独立认证配置
# 这是插件专用的管理员账号，独立于Saleor用户系统
# 用于管理支付渠道和通道的增删改操作
PLUGIN_ADMIN_USERNAME=plugin_admin
PLUGIN_ADMIN_PASSWORD=your_secure_password_here

# 会话密钥（用于加密session，请使用随机字符串）
# 生成方法：node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
PLUGIN_SESSION_SECRET=your_random_secret_key_min_32_chars

#"fatal" | "error" | "warn" | "info" | "debug" | "trace"
APP_DEBUG=info
SECRET_KEY=aaaaaaaa